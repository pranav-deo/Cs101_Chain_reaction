// Main code 1st attempt

#include <iostream.h>
#include <conio.h>
#include <string.h>
#include <vector.h>

//Function to check weightage of each cell
bool allowed(int x,int y,int value)
{
  if((x==1 && y==1) || (x==9 && y==9) || (x==1 && y==9) || (x==9 && y==1)) 
  {
    if(value<=1) return true;
    else return false;
  }
  elseif(x==1 || x==9 || y==1 || y==9)
  {
    if(value<=2) return true;
    else return false;
  }
  else()
  {
    if(value<=3) return true;
    else return false;
  }
}
